  "scripts": {
    "clean": "rm -rf ./build",
    "serve": "browser-sync start --files \"./docs/images/**/*\" --files \"./docs/md/**/*\" --files \"./docs/*.html\" --files \"./docs/data/*.tsv\" --port 3500 --server \"build/\" --server \"node_modules/\" --server \"docs/\" --no-open",
