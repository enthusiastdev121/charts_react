{
	"presets": [
		"react",
		"stage-3"
	],
	"plugins": [
		"transform-flow-strip-types",
	],
	"env": {
		"cjs": {
			"presets": [["env"]],
		},
		"es": {
			"presets": [["env", {
				"es2015": {
					"modules": false 
				}
			}]]
		},
		"watch": {
			"presets": [
				["env", {
					"targets": {
						"chrome": 58
					},
					"modules": false
				}]
			]
		},
		"server": {
			"presets": [
				["env", {
					"targets": {
						"node": 7.5
					},
					"loose": true,
				}]
			]
		}
	}
}
